{
  "name": "atom-mdtkit-lint",
  "main": "./lib/atom-mdtkit-lint",
  "version": "0.0.3",
  "description": "A plugin for Atom Linter providing an linting for the MDTKit",

  "keywords": [
    "lint",
    "MDTKit",
    "Markdown"
  ],

  "activationCommands": {
    "atom-workspace": "atom-mdtkit-lint:toggle"
  },
  
  "repository": "https://github.com/atom/atom-mdtkit-lint",
  "license": "MIT",

  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },

  "dependencies": {
    "atom-linter": "^4.6.1",
    "atom-package-deps": "^4.0.1",
    "loophole": "latest",
    "lodash": "latest",
    "mdtkit-parser" : "latest"
  },

  "package-deps": [
    "linter"
  ],

  "providedServices": {
    "linter": {
      "versions": {
        "1.0.0": "provideLinter"
      }
    }
  }
}
